{
  "name": "@packmule/sass-pack",
  "description": "Packmule pack to transpile Sass files.",
  "version": "2.0.0",
  "license": "MIT",
  "repository": "github:packmule/packmule",
  "bugs": "https://github.com/packmule/packmule/issues",
  "type": "module",
  "exports": "./module/index.js",
  "types": "module/",
  "keywords": [
    "webpack",
    "packmule",
    "packmule-pack"
  ],
  "engines": {
    "node": "14||16||18",
    "npm": ">=8",
    "pnpm": ">=7"
  },
  "author": {
    "name": "Thomas Deinhamer",
    "email": "t.deinhamer@pixelart.at",
    "url": "https://www.pixelart.at/"
  },
  "scripts": {

    "stylelint-webpack-plugin": "^3.0.1"
  },
  "devDependencies": {
    "@packmule/core": "*",
    "@types/sass": "^1.16.1",
    "@types/stylelint": "^13.13.1",
    "esbuild": "^0.14.42",
    "postcss": "^8.4.14",
    "sass": "^1.23.0",
    "stylelint": "^14.6.0",
    "webpack": "^5.73.0"
  },
  "peerDependencies": {
    "@packmule/core": "*",
    "postcss": "*",
    "sass": "*"

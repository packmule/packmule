{
    "compilerOptions": {
        "moduleResolution": "node",
        "strict": true,
        "allowUnreachableCode": true,
        "noUnusedParameters": false,
        "noUnusedLocals": true,
        "noEmitOnError": true,
        "noFallthroughCasesInSwitch": true,
        "forceConsistentCasingInFileNames": true,
        "stripInternal": true,
        "esModuleInterop": true,
        "noImplicitReturns": true,
        "skipLibCheck": true
    }
}
